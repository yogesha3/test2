<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_white"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/referrals_tabs_height"
        android:background="@drawable/bg_top_bar"
        android:id="@+id/llComposeMemberList"
        android:gravity="center_vertical"
        android:orientation="horizontal" >
        <LinearLayout
            android:id="@+id/llSelectSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_next"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:layout_alignParentLeft="true"
            android:orientation="vertical" >
            <com.app.foxhopr.custom.view.RegularTextView
                android:id="@+id/txtvSelectSearch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:text="@string/str_apply"
                android:textSize="18sp"
                style="@style/NextTextStyle"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llCancelSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_cancel"
            android:layout_alignParentRight="true"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:orientation="vertical" >
            <com.app.foxhopr.custom.view.RegularTextView
                android:id="@+id/txtvCnacelSearch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:text="@string/str_cancel"
                android:textSize="18sp"
                style="@style/NextTextStyle"
                />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/bg_select_team"
        android:layout_margin="10dp"
        android:weightSum="2"
        android:id="@+id/llSearcht"
        android:gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="50dp"
        android:orientation="horizontal" >
        <EditText
            android:id="@+id/edit_search_text"
            android:layout_width="0dp"
            android:paddingLeft="10dp"
            android:layout_weight="2"
            android:gravity="center_vertical"
            android:background="#00000000"
            android:layout_height="fill_parent"
            android:hint="@string/str_searchhint"
            android:textColor="@color/black_white"
            style="@style/dropDownNewTextStyle"
            />
        <ImageView
            android:id="@+id/imageClearSearch"
            android:layout_width="30dp"
            android:src="@drawable/ic_cross_delete"
            android:layout_height="@dimen/side_menu_icon_size"
            android:scaleType="fitXY"
            android:layout_marginRight="5dp"
            android:visibility="gone"
            android:layout_alignParentLeft="true"/>
    </LinearLayout>

